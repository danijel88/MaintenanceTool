@model List<UserViewModel>

@{
    ViewData["Title"] = "User List";
}
<a class="btn btn-primary" asp-controller="Account" asp-action="Create">Add User</a>
<div class="row">
    <div class="col-lg-12 col-md-4">
        <table class="table table-responsive table">
            <thead>
            <tr>
                <th>Email</th>
                <th>Roles</th>
            </tr>
            </thead>
            <tbody>
                @foreach (var element in Model)
                {
                    <tr>
                        <td>@element.Email</td>
                        <td>@string.Join(", ",element.Roles)</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

